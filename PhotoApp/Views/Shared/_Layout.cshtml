<!DOCTYPE html>
<html ng-app="PhotoApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>PhotoApp</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/angular")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="wrap">
    
        <div ng-include src="'/Content/html/partials/nav.html'"></div>

        @RenderBody()

    </div>

    @Scripts.Render("~/bundles/jquery", "~/bundles/jqueryui")
    @Scripts.Render("~/bundles/angular", "~/bundles/fileupload")
    @RenderSection("scripts", required: false)
</body>
</html>
